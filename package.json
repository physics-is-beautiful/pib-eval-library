{
  "name": "pib_eval_library",
  "version": "0.1.0",
  "author": "Physics is beautiful (https://www.physicsisbeautiful.com)",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "cross-env NODE_ENV=production babel src --out-dir lib --extensions \".tsx,.ts\" --copy-files",
    "build:clean": "rimraf lib/*",
    "analyze": "react-scripts build && source-map-explorer 'build/static/js/*.js'",
    "test": "CI=true react-scripts test",
    "test:watch": "react-scripts test --watch",
    "test:coverage": "CI=true react-scripts test --env=jsdom --coverage --watchAll=false --coverageReporters=json --coverageReporters=lcov --coverageReporters=text --coverageReporters=html",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --watch --noEmit",
    "lint": "eslint '**/*.@(ts|js|tsx|jsx)'",
    "lint:fix": "eslint '**/*.@(ts|js|tsx|jsx)' --fix"
  },
  "dependencies": {
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "tslib": "^1.10.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@babel/preset-typescript": "^7.8.3",
    "@types/jest": "^25",
    "@types/node": "^12",
    "@types/react": "^16.9.19",
    "@types/react-dom": "^16.9.5",
    "@typescript-eslint/eslint-plugin": "^2.19.2",
    "@typescript-eslint/parser": "^2.19.2",
    "cross-env": "^7.0.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "jest": "^25",
    "prettier": "^1.19.1",
    "react-scripts": "3.4.0",
    "rimraf": "^3.0.2",
    "sass": "^1.25.0",
    "source-map-explorer": "^2.2.2",
    "ts-jest": "^25",
    "typescript": "^3.7"
  }
}
